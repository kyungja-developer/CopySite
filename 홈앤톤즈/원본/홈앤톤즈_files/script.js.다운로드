	$(".imgListStyle2.general .list .subject").each(function(index){
		$(this).click(function(){
			if($(this).parent().parent().next().is(":hidden")) {
				$(this).parent().parent().next().stop(true, true).slideDown(500);
			} else {
				$(this).parent().parent().next().stop(true, true).slideUp(500);
			}
		})
	});


	//마이페이지 QnA
	var qnaN = -1;
	$("td.subject .qnaLink").each(function(q){
		$(this).click(function(){
			if(qnaN != q){
				$(".qnaVeiwTd").eq(qnaN).slideUp(500);
				$(".qnaVeiwTd .qnaView").eq(qnaN).slideUp(500);
				qnaN=  q; 
				$(".qnaVeiwTd").eq(qnaN).slideDown(500);
				$(".qnaVeiwTd .qnaView").eq(qnaN).slideDown(500);
			}else if(qnaN == q){
				$(".qnaVeiwTd").eq(qnaN).slideUp(500);
				$(".qnaVeiwTd .qnaView").eq(qnaN).slideUp(500);
				qnaN= -1; 
			}
		})
	});
	







