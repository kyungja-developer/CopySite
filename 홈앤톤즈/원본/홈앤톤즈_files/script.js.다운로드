	//제품 상세 - 일반 리뷰
	$(".imgListStyle2.general .list .reviewDiv2").each(function(index){
		$(this).click(function(){
			if($(this).parent().parent().next().is(":hidden")) {
				$(this).parent().parent().next().stop(true, true).slideDown(400);
			} else {
				$(this).parent().parent().next().stop(true, true).slideUp(400);
			}
		})
	});


	//제품 상세 - QnA 이벤트
	$(".slideAnwerBtn").each(function(index){
		$(this).click(function(){
			if($(this).parents("tr").next().is(":hidden")) {
				$(this).parents("tr").next().show();
			} else {
				$(this).parents("tr").next().hide();
			}
		});
	});


	//FAQ
	$(".qnaListDiv .list").each(function(index){
		$(this).children("a").click(function(){
			if($(this).next().is(":hidden")) {
				$(this).addClass("on");
				$(this).next().stop(true, true).slideDown(100);
			} else {
				$(this).removeClass("on");
				$(this).next().stop(true, true).slideUp(100);
			}
		});
	});



	//select style 이벤트
	var isSelect = false;
	var slcInterval, slcTime = 100;
	$(".slcStyle .slc").each(function(index){
		$(this).click(function(){
			if($(this).next().is(":hidden")) {
				if(isSelect) {
					$(".slcStyle").css("z-index", 1);
					$(".slcStyle .twoDep").stop(true, true).slideUp(0);
				}
				isSelect = true;
				$(this).next().stop(true, true).slideDown(400);
				$(this).parent().css("z-index", 10);
			} else {
				isSelect = false;
				$(this).next().stop(true, true).slideUp(0);
				$(this).parent().css("z-index", 1);
			}
		});

		$(this).next().find("a").click(function(){
			$(this).siblings().removeClass("on");
			$(this).addClass("on");
			$(this).parent().siblings(".slc").find(".abbreviation").text($(this).text());
			$(this).parent().stop(true, true).slideUp(400);
			
			var dataKey = $(this).attr("data-key");
			//console.log(index);
			$("#slcOption"+(index+1)+"").val(dataKey);
		});
	});

	$(".slcStyle").find("a").focusin(function(){
		clearInterval(slcInterval);
	}).focusout(function(){
		slcInterval = setInterval(function(){
			clearInterval(slcInterval);
			$(".slcStyle").css("z-index", 1);
			$(".slcStyle .twoDep").stop(true, true).slideUp(0);
		}, slcTime);
	});


	
	//searchDiv 안에 select
	var sacSlcInterval, sacSlcTime = 100;
	var isSearchSelect = false;
	$(".searchDiv .slc").click(function(){
		if($(this).next().is(":hidden")) {
			$(this).next().stop(true, true).slideDown(400);
		} else {
			$(this).next().stop(true, true).slideUp(400);
		}
	});

	$(".searchDiv .two a").click(function(){
		$(this).siblings().removeClass("on");
		$(this).addClass("on");
		$(this).parent().siblings(".slc").find(".abbreviation").text($(this).text());
		$(this).parent().stop(true, true).slideUp(400);
	});

	$(".searchDiv .slcBox").find("a").focusin(function(){
		clearInterval(sacSlcInterval);
	}).focusout(function(){
		sacSlcInterval = setInterval(function(){
			clearInterval(sacSlcInterval);
			$(".searchDiv .slcBox .two").stop(true, true).slideUp(0);
		}, sacSlcTime);
	});
	

	//css 셀렉트박스 이벤트
	$(".slcBox select").each(function(index){
		var index = $(this).find("option").index($(this).find("option:selected"));
		if(index == 0) {
			$(this).siblings(".abbreviation").text($(this).children("option:eq(0)").text());
		} else {
			$(this).siblings(".abbreviation").text($(this).children("option:eq("+index+")").text());
		}

		$(this).change(function(){
			$(this).siblings(".abbreviation").text($(this).children("option:selected").text());
		});
	});


