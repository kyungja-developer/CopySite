		$(".tabContent .rollDiv1 .rollBox .rollD").eq(index).css("left", index*1160);
	});
	
	var isStoreRollMove = false, firstStoreRollDetach, lastStoreRollDetach;
	$(".tabContent .rollDiv1 .btn > a").each(function(index){
		$(this).click(function(){
			//왼쪽
			if(!index) {
				if(!isStoreRollMove) {
					isStoreRollMove = true;
					$(".tabContent .rollDiv1 .rollBox .rollD").each(function(q){
						TweenMax.to($(".tabContent .rollDiv1 .rollBox .rollD").eq(q), 0.5, {left:parseInt($(".tabContent .rollDiv1 .rollBox .rollD").eq(q).css("left").split("p")[0]) - 1160, ease:Power3.easeOut, onComplete:function(){
							isStoreRollMove = false;
							if(q == 0) {
								firstStoreRollDetach = $(".tabContent .rollDiv1 .rollBox .rollD").eq(0).detach();
								$(".tabContent .rollDiv1 .rollBox").append(firstStoreRollDetach);
								$(".tabContent .rollDiv1 .rollBox .rollD:last").css("left", ($(".tabContent .rollDiv1 .rollBox .rollD").size()-1) * 1160);
							}
						}});
					});
				}
			} else {
				//오른쪽
				if(!isStoreRollMove) {
					isStoreRollMove = true;
					lastStoreRollDetach = $(".tabContent .rollDiv1 .rollBox .rollD:last").detach();
					$(".tabContent .rollDiv1 .rollBox").prepend(lastStoreRollDetach);
					$(".tabContent .rollDiv1 .rollBox .rollD:first").css("left", -1160);
					$(".tabContent .rollDiv1 .rollBox .rollD").each(function(q){
						TweenMax.to($(".tabContent .rollDiv1 .rollBox .rollD").eq(q), 0.5, {left:parseInt($(".tabContent .rollDiv1 .rollBox .rollD").eq(q).css("left").split("p")[0]) + 1160, ease:Power3.easeOut, onComplete:function(){
							isStoreRollMove = false;
						}});
					});
				}
			}
		});
	});

