			$(this).next().stop(true, true).slideDown(400);
		});

		$(this).mouseleave(function(){
			$(this).css("height","48px"); /* 2017-07-25  작업*/
			$(this).find(".twoD").stop(true, true).slideUp(400);
		});

		$(this).focusin(function(){
			$(this).find(".twoD").stop(true, true).slideDown(0);
		});
		$(this).focusout(function(){
			$(this).find(".twoD").stop(true, true).slideUp(300);
		});
	});
	
