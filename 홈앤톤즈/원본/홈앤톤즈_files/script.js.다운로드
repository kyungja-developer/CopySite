	//최근 본 컬러
	var selectColorMax = $(".colorChipRollDiv .colorDiv .list").size()-1;
	if(selectColorMax <= 6) {		//상품 리스트가 롤링 불가능한 숫자
		$(".colorChipRollDiv .top .buttonA button").hide();
	}
	$(".colorChipRollDiv .colorDiv .list").each(function(index){
		$(".colorChipRollDiv .colorDiv .list").eq(index).css("left", index*168);
	});

	var selectColorMove = false, firstSelectColorDetach, lastSelectColorDetach;
	$(".colorChipRollDiv .top .buttonA button").each(function(index){
		$(this).click(function(){
			if(!index) {
				if(!selectColorMove) {
					selectColorMove = true;
					$(".colorChipRollDiv .colorDiv .list").each(function(q){
						TweenMax.to($(".colorChipRollDiv .colorDiv .list").eq(q), 0.5, {left:parseInt($(".colorChipRollDiv .colorDiv .list").eq(q).css("left").split("p")[0]) - 168, ease:Power3.easeOut, onComplete:function(){
							selectColorMove = false;
							if(q == 0) {
								firstSelectColorDetach = $(".colorChipRollDiv .colorDiv .list").eq(0).detach();
								$(".colorChipRollDiv .colorDiv").append(firstSelectColorDetach);
								$(".colorChipRollDiv .colorDiv .list:last").css("left", ($(".colorChipRollDiv .colorDiv .list").size()-1) * 168);
							}
						}});
					});
				}
			} else {
				if(!selectColorMove) {
					selectColorMove = true;
					lastSelectColorDetach = $(".colorChipRollDiv .colorDiv .list:last").detach();
					$(".colorChipRollDiv .colorDiv").prepend(lastSelectColorDetach);
					$(".colorChipRollDiv .colorDiv .list:first").css("left", -168);
					$(".colorChipRollDiv .colorDiv .list").each(function(q){
						TweenMax.to($(".colorChipRollDiv .colorDiv .list").eq(q), 0.5, {left:parseInt($(".colorChipRollDiv .colorDiv .list").eq(q).css("left").split("p")[0]) + 168, ease:Power3.easeOut, onComplete:function(){
							selectColorMove = false;
						}});
					});
				}
			}
		});
	});

