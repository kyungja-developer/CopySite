		TweenMax.to($(".slidePop"), 0.5, {right:-580, ease:Power2.easeOut});
	});
	
	//메인 컬러픽 슬라이드 나오는영역 2017-07-05
	$(".roll .color").each(function(index){
		$(this).click(function(){
			$(".slidePop").css("height", $(window).height());
			TweenMax.to($(".slidePop"), 0.5, {right:0, ease:Power2.easeOut});
		});
	});
	$(".slidePop .closeBtn").click(function(){
		TweenMax.to($(".slidePop"), 0.5, {right:-580, ease:Power2.easeOut});
	});	
	
	
