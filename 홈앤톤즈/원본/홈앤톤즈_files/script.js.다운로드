		$(this).click(function(){
			$(this).next(".disneyViewPop").fadeIn(300);
			$(".dimmed").fadeIn(300);
			dViewN = q;
			if($(this).next(".disneyViewPop").find("video").size() > 0){
				myVideo = document.getElementById("myVideo" + dViewN);
				myVideo.play();
			}
		});
	});
	$(".disneyViewPop .disneyViewPopTit .disneyViewXbt").click(function(){
		$(".disneyViewPop").fadeOut(300);
		$(".dimmed").fadeOut(300);
