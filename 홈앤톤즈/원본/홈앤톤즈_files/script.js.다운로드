	$(".mainAgencyIntro .agencyInfoD .btn").each(function(index){
		$(this).click(function(){
			if(!index) {
				$(".mainAgencyIntro .agencyBg").eq(agencyInfoN).stop(true, true).fadeOut(500);
				$(".mainAgencyIntro .agencyInfoD .infoD").eq(agencyInfoN).hide();
				agencyInfoN--;
				if(agencyInfoN < 0) agencyInfoN = agencyInfoMax;
				$(".mainAgencyIntro .agencyBg").eq(agencyInfoN).stop(true, true).fadeIn(500);
				$(".mainAgencyIntro .agencyInfoD .infoD").eq(agencyInfoN).show();
			} else {
				$(".mainAgencyIntro .agencyBg").eq(agencyInfoN).stop(true, true).fadeOut(500);
				$(".mainAgencyIntro .agencyInfoD .infoD").eq(agencyInfoN).hide();
				agencyInfoN++;
				if(agencyInfoN > agencyInfoMax) agencyInfoN = 0;
				$(".mainAgencyIntro .agencyBg").eq(agencyInfoN).stop(true, true).fadeIn(500);
				$(".mainAgencyIntro .agencyInfoD .infoD").eq(agencyInfoN).show();
			}
		});
