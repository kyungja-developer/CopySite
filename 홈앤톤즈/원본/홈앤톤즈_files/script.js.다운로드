	// 2017-04-17 추가
	//주문배송조회 상세페이지 배송지변경
	jQuery(".myInfoDiv .changeAddr").each(function(index){
		jQuery(this).click(function(){
			if(jQuery(this).hasClass("on") == true){
				jQuery(this).removeClass("on");
				jQuery(".changeAddrDiv").css("display","none");
			}else if(jQuery(this).hasClass("on") == false){
				jQuery(this).addClass("on");
				jQuery(".changeAddrDiv").css("display","block");
			}else{
			}
		});
	});

	//주문배송조회 상세페이지 배송지변경-이메일
	jQuery("#emailBox").change(function(){
		if(jQuery(this).val() == "easymedia") {
			jQuery("#email2").val("easymedia.net");
		}else if(jQuery(this).val() == "naver") {
			jQuery("#email2").val("naver.com");
		}else if(jQuery(this).val() == "google") {
			jQuery("#email2").val("google.com");
		}else {
			jQuery("#email2").val("");
		}
	});
	
	//주문배송조회 상세페이지 배송지변경-배송메시지
	jQuery("#message").change(function(){
		if(jQuery(this).val() == "self") {
			jQuery(".messageInput").css("display","inline-block");
		}else {
			jQuery(".messageInput").css("display","none");
		}
	});


