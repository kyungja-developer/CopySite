$(function(){
	//console.log($(".class").css( "background-color" ).split("(")[1].split(",")[0])
	//메인비주얼 height
	$(window).resize(function(){
		//$(".mainVisual").height($(window).height());
		//$(".mainVisual .bltDiv").css("left", (1160/2) - ($(".mainVisual .bltDiv").width()/2));
		//$(".mainVisual .mouseC").css("left", (1160/2) - ($(".mainVisual .mouseC").width()/2));
		$(".mainAcademy .bltDiv").css("left", ($(window).width()/2) - ($(".mainAcademy .bltDiv").width()/2));
		$(".slidePop").css("height", $(window).innerHeight());
	});$(window).resize();

	$(window).scroll(function(){
		if($(window).scrollTop() >= 100) {
			TweenMax.to($("header.main"), 1, {backgroundColor:"rgba(45, 39, 39, 1)", ease:Power3.easeOut});
		} else {
			TweenMax.to($("header.main"), 1, {backgroundColor:"rgba(45, 32019-03-189, 39, 0)", ease:Power3.easeOut});
		}
		
	}); $(window).scroll();

	$(window).scroll(function(){
		if($(window).scrollTop() >= 100) {
			TweenMax.to($("header.sub"), 1, {backgroundColor:"rgba(45, 39, 39, 1)", ease:Power3.easeOut});
		} else {
			TweenMax.to($("header.sub"), 1, {backgroundColor:"rgba(45, 39, 39, 1)", ease:Power3.easeOut});
		}
	}); $(window).scroll();
	

	//메인비주얼 자동 롤링
	var mainRollN = 0;
	var mainMaxRollN = $(".mainVisual .visual").size()-1;
	var mainScrollInterval, mainScrollTime = 11000, mainRollImpossible = false, mainRollInterval, mainRollTime = 11000;/* 2020-07-30 수정 */
	if($(".mainVisual .visual").size()-1 <= 0) mainRollImpossible = true;
	mainRollInterval = setInterval(function(){
		if(mainRollImpossible) return;
		$(".mainVisual .visual").eq(mainRollN).stop(true, true).fadeOut(500);
		$(".mainVisual .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".mainVisual .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace("_on.png", ".png"));
		mainRollN++;
		if(mainRollN > $(".mainVisual .bltDiv .bltBt").size()-1) mainRollN = 0;
		$(".mainVisual .visual").eq(mainRollN).stop(true, true).fadeIn(500);
		$(".mainVisual .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".mainVisual .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace(".png", "_on.png"));
	}, mainRollTime);

	$(".mainVisual .bltDiv .playBt").click(function(){
		if($(this).find("img").attr("src").indexOf("stop") > -1) {
			$(this).find("img").attr("src", $(this).find("img").attr("src").replace("_stop", "_play"));
			$(this).find("img").attr("alt", "이벤트배너 재생시키기");
			clearInterval(mainRollInterval);
		} else {
			$(this).find("img").attr("src", $(this).find("img").attr("src").replace("_play", "_stop"));
			$(this).find("img").attr("alt", "이벤트배너 정지시키기");
			clearInterval(mainRollInterval);
			mainRollInterval = setInterval(function(){
				$(".mainVisual .visual").eq(mainRollN).stop(true, true).fadeOut(500);
				$(".mainVisual .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".mainVisual .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace("_on.png", ".png"));
				mainRollN++;
				if(mainRollN > $(".mainVisual .bltDiv .bltBt").size()-1) mainRollN = 0;
				$(".mainVisual .visual").eq(mainRollN).stop(true, true).fadeIn(500);
				$(".mainVisual .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".mainVisual .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace(".png", "_on.png"));
			}, mainRollTime);
		}
	});


	//메인비주얼 롤링
	var mainRollN = 0;
	$(".mainVisual .bltDiv .bltBt").each(function(index){
		$(this).click(function(){
			if(index != mainRollN) {
				$(".mainVisual .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".mainVisual .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace("_on.png", ".png"));
				$(".mainVisual .visual").eq(mainRollN).stop(true, true).fadeOut(500);
				mainRollN = index;
				$(".mainVisual .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".mainVisual .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace(".png", "_on.png"));
				$(".mainVisual .visual").eq(mainRollN).stop(true, true).fadeIn(500);
			}
		});
	});

	//메인비주얼 마우스
	$(".mainVisual .mouseC").click(function(){
		TweenMax.to($("html, body"), 0.7, {scrollTop:$(".mainEvent").offset().top - $("header").height(), ease:Power2.easeOut});
	});

	//메인이벤트 배너
	var mainEventN = 0, mainEventMax = $(".mainEvent .event").size()-1;
	$(".mainEvent button").each(function(index){
		$(this).click(function(){
			if(!index) {
				$(".mainEvent .event").eq(mainEventN).stop(true, true).fadeOut(500);
				$(".mainEvent .mainEventIndi a").eq(mainEventN).removeClass("on"); /* 2019-01-24 추가 */
				mainEventN--;
				if(mainEventN < 0) mainEventN = mainEventMax;
				$(".mainEvent .event").eq(mainEventN).stop(true, true).fadeIn(500);
				$(".mainEvent .mainEventIndi a").eq(mainEventN).addClass("on"); /* 2019-01-24 추가 */
			} else {
				$(".mainEvent .event").eq(mainEventN).stop(true, true).fadeOut(500);
				$(".mainEvent .mainEventIndi a").eq(mainEventN).removeClass("on"); /* 2019-01-24 추가 */
				mainEventN++;
				if(mainEventN > mainEventMax) mainEventN = 0;
				$(".mainEvent .event").eq(mainEventN).stop(true, true).fadeIn(500);
				$(".mainEvent .mainEventIndi a").eq(mainEventN).addClass("on");
			}
		});
	});

	//메인 비주얼 레이어팝업
	$(".mainVisual .colorProduct").each(function(q){
		$(this).click(function(){
			//$(".colorProductPop").eq(q).css("left", ($(window).width()/2) - ($(".colorProductPop").width()/2));
			//$(".colorProductPop").eq(q).css("top", ($(window).height()/2) - ($(".colorProductPop").height()/2));
			$(".colorProductPop").eq(q).stop(true, true).fadeIn(400);
			$(".mainBg").stop(true, true).fadeIn(400);
			clearInterval(mainRollInterval);
		});
	});
	$(".mainVisual .colorProductPop .xBtn").click(function(){
		$(".mainBg").trigger("click");
	});

	//colorProductPop 롤링
	


	//main mainBest
	var bestN = 0;
	$(".mainBest .bestTab a").each(function(index){
		$(this).click(function(){
			if(bestN != index) {
				$(".mainBest .bestTab a").eq(bestN).removeClass("on");
				$(".mainBest .mainBestTab").eq(bestN).hide();
				bestN = index;
				$(".mainBest .bestTab a").eq(bestN).addClass("on");
				$(".mainBest .mainBestTab").eq(bestN).show();
			}
		});
	});

	//메인 컬러 아이디어 2018-01-25 수정 시작
	var mainColorIdeaMax = $(".mainColorPick .colorPicTab .tabD .tabRollDiv .tab").size();
	// $(".mainColorPick .colorPicTab .tabD .tabRollDiv").css('width', 289*mainColorIdeaMax); 2019-10-17 삭제
	if(mainColorIdeaMax < 5) {
		$(".mainColorPick .colorPicTab .tabD .leftBtn").hide();
		$(".mainColorPick .colorPicTab .tabD .rightBtn").hide();
	};
	$(".mainColorPick .colorPicTab .tabD .tab").each(function(index){
		// $(this).css("left", index * 289);  2019-10-17 삭제
		$(this).click(function(){
			$(".mainColorPick .colorPicTab .tabD .tab").removeClass("on");
			$(this).addClass("on");
		});
	});

	// 2018-01-25 수정 끝

	var isMainColorIdeaMove = false, firstMainIdeaDetach, lastMainIdeaDetach;
	$(".mainColorPick .colorPicTab .tabD>.btn").each(function(index){
		$(this).click(function(){
			if(!index) {
				if(!isMainColorIdeaMove) {
					isMainColorIdeaMove = true;
					$(".mainColorPick .colorPicTab .tabD .tab").each(function(q){
						TweenMax.to($(".mainColorPick .colorPicTab .tabD .tab").eq(q), 0.5, {left:parseInt($(".mainColorPick .colorPicTab .tabD .tab").eq(q).css("left").split("p")[0]) - 290, ease:Power3.easeOut, onComplete:function(){
							isMainColorIdeaMove = false;
							if(q == 0) {
								firstMainIdeaDetach = $(".mainColorPick .colorPicTab .tabD .tab").eq(0).detach();
								$(".mainColorPick .colorPicTab .tabD .tabRollDiv").append(firstMainIdeaDetach);
								$(".mainColorPick .colorPicTab .tabD .tab:last").css("left", ($(".mainColorPick .colorPicTab .tabD .tab").size()-1) * 290);
							}
						}});
					});
				}
			} else {
				if(!isMainColorIdeaMove) {
					isMainColorIdeaMove = true;
					lastMainIdeaDetach = $(".mainColorPick .colorPicTab .tabD .tab:last").detach();
					$(".mainColorPick .colorPicTab .tabD .tabRollDiv").prepend(lastMainIdeaDetach);
					$(".mainColorPick .colorPicTab .tabD .tab:first").css("left", -290);
					$(".mainColorPick .colorPicTab .tabD .tab").each(function(q){
						TweenMax.to($(".mainColorPick .colorPicTab .tabD .tab").eq(q), 0.5, {left:parseInt($(".mainColorPick .colorPicTab .tabD .tab").eq(q).css("left").split("p")[0]) + 290, ease:Power3.easeOut, onComplete:function(){
							isMainColorIdeaMove = false;
						}});
					});
				}
			}
		});
	});

	
	/*
	//메인 colorPlusBtn
	$(".colorPlusBtn").each(function(q){
		$(this).toggle(function(){
			$(".colorPlusPop").eq(q).css("display","block");
			$(this).find("img").attr("src", $(this).find("img").attr("src").replace("More", "Close"));
		}, function(){
			$(".colorPlusPop").eq(q).css("display","none");
			$(this).find("img").attr("src", $(this).find("img").attr("src").replace("Close", "More"));
		});
	});
	*/
	//메인 colorPlusBtn
	//2017-07-12 토글이벤트가 적용이 안되어 클릭이벤트로 변경 안진용
	$(".colorPlusBtn").each(function(q){
		$(this).click(function(){
			if($(".colorPlusPop").eq(q).css("display")=="none"){
				$(".colorPlusPop").eq(q).css("display","block");
				$(this).find("img").attr("src", $(this).find("img").attr("src").replace("More", "Close"));
			}else{
				$(".colorPlusPop").eq(q).css("display","none");
				$(this).find("img").attr("src", $(this).find("img").attr("src").replace("Close", "More"));
			}
		})
	});




	//메인 COLOR / PRODUCT 팝업
	var cpRollWidth = 440, cpRollMax1 = $(".mainVisual .colorProductPop1 .rollDiv .rollItem").size()-1;
	if(cpRollMax1 < 2) {		//상품 리스트가 롤링 불가능한 숫자
		$(".mainVisual .colorProductPop1 .maskD .btn").hide();
	}

	$(".mainVisual .colorProductPop1 .rollDiv .rollItem").each(function(index){
		$(".mainVisual .colorProductPop1 .rollDiv .rollItem").eq(index).css("left", index*cpRollWidth);
	});

	var isCpRollMove1 = false, firstCpRollDetach1, lastCpRollDetach1;
	$(".mainVisual .colorProductPop1 .maskD .btn").each(function(index){
		$(this).click(function(){
			if(!index) {
				if(!isCpRollMove1) {
					isCpRollMove1 = true;
					$(".mainVisual .colorProductPop1 .rollDiv .rollItem").each(function(q){
						TweenMax.to($(".mainVisual .colorProductPop1 .rollDiv .rollItem").eq(q), 0.5, {left:parseInt($(".mainVisual .colorProductPop1 .rollDiv .rollItem").eq(q).css("left").split("p")[0]) - 440, ease:Power3.easeOut, onComplete:function(){
							isCpRollMove1 = false;
							if(q == 0) {
								firstCpRollDetach1 = $(".mainVisual .colorProductPop1 .rollDiv .rollItem").eq(0).detach();
								$(".mainVisual .colorProductPop1 .rollDiv").append(firstCpRollDetach1);
								$(".mainVisual .colorProductPop1 .rollDiv .rollItem:last").css("left", ($(".mainVisual .colorProductPop1 .rollDiv .rollItem").size()-1) * 440);
							}
						}});
					});
				}
			} else {
				if(!isCpRollMove1) {
					isCpRollMove1 = true;
					lastCpRollDetach1 = $(".mainVisual .colorProductPop1 .rollDiv .rollItem:last").detach();
					$(".mainVisual .colorProductPop1 .rollDiv").prepend(lastCpRollDetach1);
					$(".mainVisual .colorProductPop1 .rollDiv .rollItem:first").css("left", -440);
					$(".mainVisual .colorProductPop1 .rollDiv .rollItem").each(function(q){
						TweenMax.to($(".mainVisual .colorProductPop1 .rollDiv .rollItem").eq(q), 0.5, {left:parseInt($(".mainVisual .colorProductPop1 .rollDiv .rollItem").eq(q).css("left").split("p")[0]) + 440, ease:Power3.easeOut, onComplete:function(){
							isCpRollMove1 = false;
						}});
					});
				}
			}
		});
	});

	//메인 COLOR / PRODUCT 팝업
	var cpRollMax2 = $(".mainVisual .colorProductPop2 .rollDiv .rollItem").size()-1;
	if(cpRollMax2 <= 0) {		//상품 리스트가 롤링 불가능한 숫자
		$(".mainVisual .colorProductPop2 .maskD .btn").hide();
	}
	$(".mainVisual .colorProductPop2 .rollDiv .rollItem").each(function(index){
		$(".mainVisual .colorProductPop2 .rollDiv .rollItem").eq(index).css("left", index*cpRollWidth);
	});

	var isCpRollMove2 = false, firstCpRollDetach2, lastCpRollDetach2;
	$(".mainVisual .colorProductPop2 .maskD .btn").each(function(index){
		$(this).click(function(){
			if(!index) {
				if(!isCpRollMove2) {
					isCpRollMove2 = true;
					$(".mainVisual .colorProductPop2 .rollDiv .rollItem").each(function(q){
						TweenMax.to($(".mainVisual .colorProductPop2 .rollDiv .rollItem").eq(q), 0.5, {left:parseInt($(".mainVisual .colorProductPop2 .rollDiv .rollItem").eq(q).css("left").split("p")[0]) - 440, ease:Power3.easeOut, onComplete:function(){
							isCpRollMove2 = false;
							if(q == 0) {
								firstCpRollDetach2 = $(".mainVisual .colorProductPop2 .rollDiv .rollItem").eq(0).detach();
								$(".mainVisual .colorProductPop2 .rollDiv").append(firstCpRollDetach2);
								$(".mainVisual .colorProductPop2 .rollDiv .rollItem:last").css("left", ($(".mainVisual .colorProductPop2 .rollDiv .rollItem").size()-1) * 440);
							}
						}});
					});
				}
			} else {
				if(!isCpRollMove2) {
					isCpRollMove2 = true;
					lastCpRollDetach2 = $(".mainVisual .colorProductPop2 .rollDiv .rollItem:last").detach();
					$(".mainVisual .colorProductPop2 .rollDiv").prepend(lastCpRollDetach2);
					$(".mainVisual .colorProductPop2 .rollDiv .rollItem:first").css("left", -440);
					$(".mainVisual .colorProductPop2 .rollDiv .rollItem").each(function(q){
						TweenMax.to($(".mainVisual .colorProductPop2 .rollDiv .rollItem").eq(q), 0.5, {left:parseInt($(".mainVisual .colorProductPop2 .rollDiv .rollItem").eq(q).css("left").split("p")[0]) + 440, ease:Power3.easeOut, onComplete:function(){
							isCpRollMove2 = false;
						}});
					});
				}
			}
		});
	});

	//메인 COLOR / PRODUCT 팝업
	var cpRollMax3 = $(".mainVisual .colorProductPop3 .rollDiv .rollItem").size()-1;
	if(cpRollMax3 <= 0) {		//상품 리스트가 롤링 불가능한 숫자
		$(".mainVisual .colorProductPop3 .maskD .btn").hide();
	}
	$(".mainVisual .colorProductPop3 .rollDiv .rollItem").each(function(index){
		$(".mainVisual .colorProductPop3 .rollDiv .rollItem").eq(index).css("left", index*cpRollWidth);
