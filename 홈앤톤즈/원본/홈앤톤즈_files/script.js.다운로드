					isColorPicViewMove = true;
					$(".colorIdeaView .rollDiv .list").each(function(q){
						TweenMax.to($(".colorIdeaView .rollDiv .list").eq(q), 0.3, {left:parseInt($(".colorIdeaView .rollDiv .list").eq(q).css("left").split("p")[0]) - 219, ease:Power3.easeOut, onComplete:function(){
							isColorPicViewMove = false;
							if(q == 0) colorPicViewDetachFirst = $(".colorIdeaView .rollDiv .list").eq(0).detach();
							$(".colorIdeaView .rollDiv").append(colorPicViewDetachFirst);
							$(".colorIdeaView .rollDiv .list:last").css("left", 219 * colorPicViewColorMax);
						}});
					});
				}
			} else {
				if(!isColorPicViewMove) {
					isColorPicViewMove = true;
					colorPicViewDetachLast = $(".colorIdeaView .rollDiv .list:last").detach();
					$(".colorIdeaView .rollDiv").prepend(colorPicViewDetachLast);
					$(".colorIdeaView .rollDiv .list:first").css("left", -219);
					$(".colorIdeaView .rollDiv .list").each(function(q){
						TweenMax.to($(".colorIdeaView .rollDiv .list").eq(q), 0.3, {left:parseInt($(".colorIdeaView .rollDiv .list").eq(q).css("left").split("p")[0]) + 219, ease:Power3.easeOut, onComplete:function(){
							isColorPicViewMove = false;
						}});
					});
				}
			}
		});
