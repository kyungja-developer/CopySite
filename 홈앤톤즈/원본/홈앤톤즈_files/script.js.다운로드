		$(this).hover(function(){
			$(this).find(".overDiv").height($(this).find(".img").height() - 60);
			$(this).find(".overDiv").stop(true, true).fadeIn(300);
		}, function(){
			$(this).find(".overDiv").stop(true, true).fadeOut(300);
		});
	});
	
	//2017-06-23 로딩 이미지 높이값
	$(window).resize(function(){
		$(".loading").height($(window).height());
		//$(".loading").parents("body").css("overflow","hidden")
		//$("body").css("overflow","hidden")
		$(".storeBanner .rollDiv").css("height",$(".storeBanner .rollDiv .rollD").height());
	});$(window).resize();

	//2017-10-30 마감된강좌 앵커이동
	$(".soldout").click(function(){
		TweenMax.to($("html, body"), 1, {"scrollTop":$("#soldout").offset().top-50, ease:Power3.easeOut});
	})

	// 2020-11-30 동영상 종료 후 재생버튼 보이게 추가
	$(".megaIntroDiv4 .conDiv video").on("ended", function(){
		$(".megaNoteArea .conDiv .end_video").fadeIn(100);
		//$(".megaIntroDiv4 .conDiv video")[0].load(); 2020-12-01 삭제
	});

	// 2020-11-30 메가노트 비디오 재생버튼
	$(".megaNoteArea .conDiv .end_video a").click(function(){
		$(".megaNoteArea .conDiv .end_video").fadeOut();
		$(".megaIntroDiv4 .conDiv video")[0].play();
	});

	/* 2020-12-09 펫 러브 컬러 차트 이름 보이기 추가 */
	$(".colorChartChkDiv3 .chkBox label").on("click", function(){
		if(!$(this).hasClass("on")){
			$(this).addClass("on");
			$(".petChart .colorDiv").addClass("on");
		}else{
			$(this).removeClass("on");
			$(".petChart .colorDiv").removeClass("on");
		}
	});

	$(".colorChartChkDiv3 .chkBox #chk1").on("click", function(){
		if(!$(this).hasClass("on")){
			$(this).addClass("on");
			$(".petChart .colorDiv").addClass("on");
		}else{
			$(this).removeClass("on");
			$(".petChart .colorDiv").removeClass("on");
		}
	});
	/* // 2020-12-09 추가 */
});


//상품 주문 - 개수 설정
function initCount()
{
	$(".countD").each(function(index){
		$(this).find(".cntBt a").each(function(q){
			$(this).click(function(){
				if(!q) {
					if(parseInt($(this).parent().prev().find("input").val())+1 <= 99) $(this).parent().prev().find("input").val(parseInt($(this).parent().prev().find("input").val())+1);
				} else {
					if(parseInt($(this).parent().prev().find("input").val())-1 >= 1) $(this).parent().prev().find("input").val(parseInt($(this).parent().prev().find("input").val())-1);
				}
			})
		});
	});
};


function getBrowser()
{
	var Browser = {chk : navigator.userAgent.toLowerCase()}
		Browser = {
			ie : Browser.chk.indexOf('msie') != -1,
			ie6 : Browser.chk.indexOf('msie 6') != -1,
			ie7 : Browser.chk.indexOf('msie 7') != -1,
			ie8 : Browser.chk.indexOf('msie 8') != -1,
			ie9 : Browser.chk.indexOf('msie 9') != -1,
			ie10 : Browser.chk.indexOf('msie 10') != -1,
			opera : !!window.opera,
			safari : Browser.chk.indexOf('safari') != -1,
			safari3 : Browser.chk.indexOf('applewebkir/5') != -1,
			mac : Browser.chk.indexOf('mac') != -1,
			chrome : Browser.chk.indexOf('chrome') != -1,
			firefox : Browser.chk.indexOf('firefox') != -1
		}

		 if(Browser.firefox) {
			$(".consultingReservPop .step .fixedTable").css("top", 85);
		} else {
			// 그 외 실행할 코드
		}
};


var layer = function (val){
  jQuery(".blackBg").stop(true, true).fadeIn(400);
//  jQuery(".layerPopType").css("display","block");
  jQuery(".layerPopType").css("top", $(window).scrollTop()+50);
	jQuery(".layerPopType").css("left", $(window).width()/2);
  jQuery(".layerPopType").css("margin-left", -$(".layerPopType").width()/2);
  jQuery(".orderCouponPop").css("margin-left", -$(".orderCouponPop").width()/2);
  jQuery(".emailAlertPop").css("margin-left", -$(".emailAlertPop").width()/2);
  jQuery(".body").css("overflow", "auto");
  jQuery(".body").css("max-height", "620px");
  jQuery(".popBody").css("overflow", "auto");
  jQuery(".popBody").css("max-height", "620px");
  jQuery(".bodyCon").css("padding-bottom", "35px");
  if(val!=""&&typeof val!="undefined"){
	//jQuery("body").css("overflow", "hidden");
  }else{
	jQuery("body").css("overflow", "hidden");
  }
};

/* 2019-07-08 추가  - 더보기 버튼 클릭시 데이터 불러온 다음 재호출 */
function moreClick(){
	//메인 colorpick//6041969216
	if($(".mainGrid").size() != 0) {
		$(".mainGrid").isotope({
		  itemSelector: '.mainGrid .item',
		  masonry: {columnWidth:368}
		});
		$(window).resize();
	}

	//시공사례 갤러리//6041969216
	if($(".grid").size() != 0) {
		$(".grid").isotope({
		  itemSelector: '.grid .list',
		  masonry: {columnWidth:393}
		});
		$(window).resize();
	}
	
	//컬러리뷰 2017-03-27 추가
	if($(".mainGrid2").size() != 0) {
		$(".mainGrid2").isotope({
		  itemSelector: '.mainGrid2 .item',
		  masonry: {columnWidth:393}
		});
		$(window).resize();
	}

	//컬러아이디어 팝업 2017-03-28 추가
	if($(".mainGrid3").size() != 0) {
		$(".mainGrid3").isotope({
		  itemSelector: '.mainGrid3 .item',
		  //masonry: {columnWidth:255}
		});
		$(window).resize();
	}
}
/* // 2019-07-08 추가 */
