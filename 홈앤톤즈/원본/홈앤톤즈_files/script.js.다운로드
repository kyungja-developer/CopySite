	});
	$(".summerDiv").hover(function(){
		TweenMax.to($(".catalogDiv.season .catalogBg"), catalogSpeed, {opacity:1, ease:Power2.easeOut});
	}, function(){
		TweenMax.to($(".catalogDiv.season .catalogBg"), catalogSpeed, {opacity:0, ease:Power2.easeOut});
	});
	*/


	//paint submain 페인트 서브메인
	$(".rollContainer .rollDiv").each(function(q){
		if($(this).children(".roll").size() < 2 ){
		$(this).parent().children(".btn").hide();
		}
	});
	var paintRollMove = false;
	$(".paintMainBrandDetail .rollContainer").each(function(q){
		$(this).find(".rollDiv>.roll:first").addClass("on");
		$(this).find(".btn").each(function(k){
			$(this).click(function(){
				if(!k) {
					if(!paintRollMove){
						paintRollMove = true;
						var paintSubRollNum = 0;
						paintSubRollNum = $(this).siblings(".rollDiv").find(".roll.on").index();
						TweenMax.to($(this).siblings(".rollDiv").find(".roll").eq(paintSubRollNum), 0.5, {left:-370, ease:Power2.easeOut});
						$(this).siblings(".rollDiv").find(".roll").eq(paintSubRollNum).removeClass("on");
						paintSubRollNum++;
						if(paintSubRollNum > $(this).siblings(".rollDiv").find(".roll").size()-1) paintSubRollNum = 0;
						console.log($(this).siblings(".rollDiv").find(".roll").size()-1);
						$(this).siblings(".rollDiv").find(".roll").eq(paintSubRollNum).addClass("on");
						TweenMax.to($(this).siblings(".rollDiv").find(".roll").eq(paintSubRollNum), 0, {left:370, ease:Power2.easeOut});
						TweenMax.to($(this).siblings(".rollDiv").find(".roll").eq(paintSubRollNum), 0.5, {left:0, ease:Power2.easeOut, onComplete: function(){
							paintRollMove = false;
						}});
					}
				} else {
					if(!paintRollMove){
						paintRollMove = true;
						var paintSubRollNum = 0;
						paintSubRollNum = $(this).siblings(".rollDiv").find(".roll.on").index();
						TweenMax.to($(this).siblings(".rollDiv").find(".roll").eq(paintSubRollNum), 0.5, {left:370, ease:Power2.easeOut});
						$(this).siblings(".rollDiv").find(".roll").eq(paintSubRollNum).removeClass("on");
						paintSubRollNum--;
						if(paintSubRollNum < 0) paintSubRollNum = $(this).siblings(".rollDiv").find(".roll").size()-1;
						console.log($(this).siblings(".rollDiv").find(".roll").size()-1);
						$(this).siblings(".rollDiv").find(".roll").eq(paintSubRollNum).addClass("on");
						TweenMax.to($(this).siblings(".rollDiv").find(".roll").eq(paintSubRollNum), 0, {left:-370, ease:Power2.easeOut});
						TweenMax.to($(this).siblings(".rollDiv").find(".roll").eq(paintSubRollNum), 0.5, {left:0, ease:Power2.easeOut, onComplete: function(){
							paintRollMove = false;
						}});
					}
				}
			});
		});
	});


	//order 주문/결제
	$(".orderCompDiv .agreeAcoD .slideBtn").each(function(q){
		$(this).click(function(){
			if($(".orderCompDiv .agreeAcoView").eq(q).is(":hidden")) {
				$(".orderCompDiv .agreeAcoView").eq(q).stop(true, true).slideDown(300);
				$(".orderCompDiv .agreeAcoD .slideBtn").eq(q).addClass("on");
				$(".orderCompDiv .agreeAcoD .slideBtn span").eq(q).text("내용닫기");

				$(".scrollBox").mCustomScrollbar("destroy");
				$(".scrollBox").mCustomScrollbar({});
			} else {
				$(".orderCompDiv .agreeAcoView").eq(q).stop(true, true).slideUp(300);
				$(".orderCompDiv .agreeAcoD .slideBtn").eq(q).removeClass("on");
				$(".orderCompDiv .agreeAcoD .slideBtn span").eq(q).text("내용열기");
			}
		});
	});

