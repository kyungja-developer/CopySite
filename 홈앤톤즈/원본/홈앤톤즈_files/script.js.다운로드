	//main 강의 롤링
	var admBannerN = 0, admBannerMax = $(".mainAcademyBanner .packageLoc").size()-1;
	if(admBannerMax == 0) {
		$(".mainAcademyBanner .btnBox .bothBtn").hide();
	}
	$(".mainAcademyBanner .btnBox .bothBtn").each(function(index){
		$(this).click(function(){
			if(!index) {
				TweenMax.to($(".packageLoc").eq(admBannerN), 1, {left:-1160, ease:Power2.easeOut});
				admBannerN++;
				if(admBannerN > admBannerMax) admBannerN = 0;
				TweenMax.to($(".packageLoc").eq(admBannerN), 0, {left:1160, ease:Power2.easeOut});
				TweenMax.to($(".packageLoc").eq(admBannerN), 1, {left:0, ease:Power2.easeOut});
			} else {
				TweenMax.to($(".packageLoc").eq(admBannerN), 1, {left:1160, ease:Power2.easeOut});
				admBannerN--;
				if(admBannerN < 0) admBannerN = admBannerMax;
				TweenMax.to($(".packageLoc").eq(admBannerN), 0, {left:-1160, ease:Power2.easeOut});
				TweenMax.to($(".packageLoc").eq(admBannerN), 1, {left:0, ease:Power2.easeOut});
			}
		});
	});


