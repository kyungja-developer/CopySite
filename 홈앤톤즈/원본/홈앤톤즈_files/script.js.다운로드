	//아이디 찾기
	jQuery("#findEmail").change(function(){
		if(jQuery(this).val() == "email") {
			jQuery(".inputInformation").css("display","block");
			jQuery(".idBox .btnBox").css("display","block");
			jQuery(".idBox .btnBox2").css("display","none");
		}  else {
		}
	});

	jQuery("#findPhone").change(function(){
		if(jQuery(this).val() == "phone") {
			jQuery(".inputInformation").css("display","none");
			jQuery(".idBox .btnBox").css("display","none");
			jQuery(".idBox .btnBox2").css("display","block");
		}  else {
		}
	});

	//마이페이지 서브메인 주문현황 탭
	var orderN = 0;
	$(".orderDiv .tabs a").each(function(index){
		$(this).click(function(){
			if(orderN != index) {
				$(".orderDiv .tabs a").eq(orderN).removeClass("on");
				$(".orderDiv .tabCon").eq(orderN).hide();
				orderN = index;
				$(".orderDiv .tabs a").eq(orderN).addClass("on");
				$(".orderDiv .tabCon").eq(orderN).show();
			}
		});
	});

	//마이페이지 서브메인 서비스신청현황 탭
	var serviceN = 0;
	$(".serviceDiv .tabs a").each(function(index){
		$(this).click(function(){
			if(serviceN != index) {
				$(".serviceDiv .tabs a").eq(serviceN).removeClass("on");
				$(".serviceDiv .tabCon").eq(serviceN).hide();
				serviceN = index;
				$(".serviceDiv .tabs a").eq(serviceN).addClass("on");
				$(".serviceDiv .tabCon").eq(serviceN).show();
