	//서브 상단 이벤트존 롤링, interval
	var eventRollNum = 0;
	var eventRollMax = $(".eventRollDiv .event").size() -1;
	var isEventRollImpossible = false, eventRollInterval, eventRollTime = 6000;
	if($(".eventRollDiv .event").size()-1 <= 0) isEventRollImpossible = true;
	eventRollInterval = setInterval(function(){
		if(isEventRollImpossible) return;
		$(".eventRollDiv .event").eq(eventRollNum).stop(true, true).fadeOut(400);
		$(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src", $(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src").replace("_on.png", ".png"));
		eventRollNum++;
		if(eventRollNum > $(".eventRollDiv .bltBox .blt").size()-1) eventRollNum = 0;
		$(".eventRollDiv .event").eq(eventRollNum).stop(true, true).fadeIn(400);
		$(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src", $(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src").replace(".png", "_on.png"));
	}, eventRollTime);
	$(".eventRollDiv .event").each(function(index){
		$(this).hide();
		$(".eventRollDiv .event").eq(0).show();
	});
	$(".eventRollDiv .bltBox .blt").each(function(index){
		$(this).click(function(){
			if(eventRollNum != index) {
				if(isEventRollImpossible) return;
				$(".eventRollDiv .event").eq(eventRollNum).stop(true, true).fadeOut(400);
				$(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src", $(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src").replace("_on.png", ".png"));
				eventRollNum = index;
				$(".eventRollDiv .event").eq(eventRollNum).stop(true, true).fadeIn(400);
				$(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src", $(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src").replace(".png", "_on.png"));
			}
		});
	});
	$(".eventRollDiv .bltBox .ps").click(function(){
		if($(this).find("img").attr("src").indexOf("stop") > -1) {
			$(this).find("img").attr("src", $(this).find("img").attr("src").replace("_stop", "_play"));
			$(this).find("img").attr("alt", "이벤트배너 재생시키기");
			clearInterval(eventRollInterval);
		} else {
			$(this).find("img").attr("src", $(this).find("img").attr("src").replace("_play", "_stop"));
			$(this).find("img").attr("alt", "이벤트배너 정지시키기");
			clearInterval(eventRollInterval);
			eventRollInterval = setInterval(function(){
				$(".eventRollDiv .event").eq(eventRollNum).stop(true, true).fadeOut(400);
				$(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src", $(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src").replace("_on.png", ".png"));
				eventRollNum++;
				if(eventRollNum > $(".eventRollDiv .bltBox .blt").size()-1) eventRollNum = 0;
				$(".eventRollDiv .event").eq(eventRollNum).stop(true, true).fadeIn(400);
				$(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src", $(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src").replace(".png", "_on.png"));
			}, eventRollTime);
		}
	});
	
	$(".eventRollDiv .leftBtn").click(function(){
		if(isEventRollImpossible) return;
		$(".eventRollDiv .event").eq(eventRollNum).stop(true, true).fadeOut(400);
		$(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src", $(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src").replace("_on.png", ".png"));
		eventRollNum--;
		if(eventRollNum < 0) eventRollNum = eventRollMax;
		$(".eventRollDiv .event").eq(eventRollNum).stop(true, true).fadeIn(400);
		$(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src", $(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src").replace(".png", "_on.png"));
	});
	$(".eventRollDiv .rightBtn").click(function(){
		if(isEventRollImpossible) return;
		$(".eventRollDiv .event").eq(eventRollNum).stop(true, true).fadeOut(400);
		$(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src", $(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src").replace("_on.png", ".png"));
		eventRollNum++;
		if(eventRollNum > eventRollMax) eventRollNum = 0;
		$(".eventRollDiv .event").eq(eventRollNum).stop(true, true).fadeIn(400);
		$(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src", $(".eventRollDiv .bltBox .blt").eq(eventRollNum).find("img").attr("src").replace(".png", "_on.png"));
	});


	//베스트 상품
	var bestGoodsWidth = 260, bestGoodsMax = $(".bestGoodsRoll .goods .list").size()-1;
	if(bestGoodsMax <= 3) {		//상품 리스트가 롤링 불가능한 숫자
		$(".bestGoodsRoll .goodsRoll>a").hide();
	}
	$(".bestGoodsRoll .goods .list").each(function(index){
		$(".bestGoodsRoll .goods .list").eq(index).css("left", index*bestGoodsWidth);
		/*
		TweenMax.to($(".bestGoodsRoll .goods .list .overBox").eq(index), 0, {top:200, ease:Power3.easeOut});
		
		$(this).hover(function(){
			TweenMax.to($(this).find(".overBox"), 0.6, {top:0, ease:Power3.easeOut});
		}, function(){
			TweenMax.to($(this).find(".overBox"), 0.6, {top:200, ease:Power3.easeOut});
		});
		*/
	});
	var isBestGoodsMove = false, firstBestGoodsDetach, lastBestGoodsDetach;
	$(".bestGoodsRoll .goodsRoll>a").each(function(index){
		$(this).click(function(){
			if(!index) {
				if(!isBestGoodsMove) {
					isBestGoodsMove = true;
					$(".bestGoodsRoll .goods .list").each(function(q){
						TweenMax.to($(".bestGoodsRoll .goods .list").eq(q), 0.5, {left:parseInt($(".bestGoodsRoll .goods .list").eq(q).css("left").split("p")[0]) - 260, ease:Power3.easeOut, onComplete:function(){
							isBestGoodsMove = false;
							if(q == 0) {
								firstBestGoodsDetach = $(".bestGoodsRoll .goods .list").eq(0).detach();
								$(".bestGoodsRoll .goods").append(firstBestGoodsDetach);
								$(".bestGoodsRoll .goods .list:last").css("left", ($(".bestGoodsRoll .goods .list").size()-1) * 260);
							}
						}});
					});
				}
			} else {
				if(!isBestGoodsMove) {
					isBestGoodsMove = true;
					lastBestGoodsDetach = $(".bestGoodsRoll .goods .list:last").detach();
					$(".bestGoodsRoll .goods").prepend(lastBestGoodsDetach);
					$(".bestGoodsRoll .goods .list:first").css("left", -260);
					$(".bestGoodsRoll .goods .list").each(function(q){
						TweenMax.to($(".bestGoodsRoll .goods .list").eq(q), 0.5, {left:parseInt($(".bestGoodsRoll .goods .list").eq(q).css("left").split("p")[0]) + 260, ease:Power3.easeOut, onComplete:function(){
							isBestGoodsMove = false;
						}});
					});
				}
			}
		});
	});

