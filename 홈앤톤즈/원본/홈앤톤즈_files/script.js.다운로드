							isCpRollMove3 = false;
							if(q == 0) {
								firstCpRollDetach3 = $(".mainVisual .colorProductPop3 .rollDiv .rollItem").eq(0).detach();
								$(".mainVisual .colorProductPop3 .rollDiv").append(firstCpRollDetach3);
								$(".mainVisual .colorProductPop3 .rollDiv .rollItem:last").css("left", ($(".mainVisual .colorProductPop3 .rollDiv .rollItem").size()-1) * 440);
							}
						}});
					});
				}
			} else {
				if(!isCpRollMove3) {
					isCpRollMove3 = true;
					lastCpRollDetach3 = $(".mainVisual .colorProductPop3 .rollDiv .rollItem:last").detach();
					$(".mainVisual .colorProductPop3 .rollDiv").prepend(lastCpRollDetach3);
					$(".mainVisual .colorProductPop3 .rollDiv .rollItem:first").css("left", -440);
					$(".mainVisual .colorProductPop3 .rollDiv .rollItem").each(function(q){
