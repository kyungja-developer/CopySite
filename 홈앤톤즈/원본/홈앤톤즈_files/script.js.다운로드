	//메인 COLOR / PRODUCT 팝업
	var cpRollMax4 = $(".mainVisual .colorProductPop4 .rollDiv .rollItem").size()-1;
	if(cpRollMax4 <= 0) {		//상품 리스트가 롤링 불가능한 숫자
