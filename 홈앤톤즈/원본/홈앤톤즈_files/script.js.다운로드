	//시공서비스 범위
	var buildGoodsN = -1;
	$(".buildGoodsDiv2 button").each(function(index){
		$(this).click(function(){
			if(buildGoodsN == -1) {
				$(".buildGoodsDiv2 button").eq(index).find("img").attr("src", $(".buildGoodsDiv2 button").eq(index).find("img").attr("src").replace(".gif", "_on.gif"));
				buildGoodsN = index;
				$(".buildGoodsDiv2 .right").find("img").attr("src", "/common/images/icon/buildGoodsDiv2_imgType"+(index+2)+".jpg");
			} else {
				if(index != buildGoodsN) {
					$(".buildGoodsDiv2 button").eq(buildGoodsN).find("img").attr("src", $(".buildGoodsDiv2 button").eq(buildGoodsN).find("img").attr("src").replace("_on.gif", ".gif"));
					buildGoodsN = index;
					$(".buildGoodsDiv2 button").eq(buildGoodsN).find("img").attr("src", $(".buildGoodsDiv2 button").eq(buildGoodsN).find("img").attr("src").replace(".gif", "_on.gif"));
					$(".buildGoodsDiv2 .right").find("img").attr("src", "/common/images/icon/buildGoodsDiv2_imgType"+(index+2)+".jpg");
				} else {
					$(".buildGoodsDiv2 button").eq(buildGoodsN).find("img").attr("src", $(".buildGoodsDiv2 button").eq(buildGoodsN).find("img").attr("src").replace("_on.gif", ".gif"));
					buildGoodsN = -1;
					$(".buildGoodsDiv2 .right").find("img").attr("src", "/common/images/icon/buildGoodsDiv2_imgType1.jpg");
				}
			}
		});
	});


	//시공사례 상세페이지
	var buildExpViewColorMax = $(".buildExpDiv3 .rollD .chip").size()-1;
	var buildExpViewDetachFirst, buildExpViewDetachLast;
	var isBuildExpViewMove = false;
	var buildExpViewChipW = 75;
	if(buildExpViewColorMax <= 3) {
		$(".buildExpDiv3 .colorRollDiv>button").hide();
		$(".buildExpDiv3 .rollD").css("left", 0);
	}
	$(".buildExpDiv3 .rollD .chip").each(function(index){
		$(this).css("left", index * buildExpViewChipW);
	});
	$(".buildExpDiv3 .colorRollDiv>button").each(function(index){
		$(this).click(function(){
			if(!index) {
				if(!isBuildExpViewMove) {
					isBuildExpViewMove = true;
					$(".buildExpDiv3 .rollD .chip").each(function(q){
						TweenMax.to($(".buildExpDiv3 .rollD .chip").eq(q), 0.3, {left:parseInt($(".buildExpDiv3 .rollD .chip").eq(q).css("left").split("p")[0]) - buildExpViewChipW, ease:Power3.easeOut, onComplete:function(){
							isBuildExpViewMove = false;
							if(q == 0) buildExpViewDetachFirst = $(".buildExpDiv3 .rollD .chip").eq(0).detach();
							$(".buildExpDiv3 .rollD").append(buildExpViewDetachFirst);
							$(".buildExpDiv3 .rollD .chip:last").css("left", buildExpViewChipW * buildExpViewColorMax);
						}});
					});
				}
			} else {
				if(!isBuildExpViewMove) {
					isBuildExpViewMove = true;
					buildExpViewDetachLast = $(".buildExpDiv3 .rollD .chip:last").detach();
					$(".buildExpDiv3 .rollD").prepend(buildExpViewDetachLast);
					$(".buildExpDiv3 .rollD .chip:first").css("left", -buildExpViewChipW);
					$(".buildExpDiv3 .rollD .chip").each(function(q){
						TweenMax.to($(".buildExpDiv3 .rollD .chip").eq(q), 0.3, {left:parseInt($(".buildExpDiv3 .rollD .chip").eq(q).css("left").split("p")[0]) + buildExpViewChipW, ease:Power3.easeOut, onComplete:function(){
							isBuildExpViewMove = false;
						}});
					});
				}
			}
		});
	});


	//color idea 상세페이지
	var colorPicViewColorMax = $(".colorIdeaView .rollDiv .list").size()-1;
	var colorPicViewDetachFirst, colorPicViewDetachLast;
	var isColorPicViewMove = false;
	if(colorPicViewColorMax < 3) {
		$(".colorIdeaView .rollCont .btn").hide();
	}
	//colorIdeaView
	$(".colorIdeaView .rollDiv .list").each(function(index){
		$(this).css("left", index * 219);
	});
	$(".colorIdeaView .rollCont .btn").each(function(index){
		$(this).click(function(){
			if(!index) {
				if(!isColorPicViewMove) {
					isColorPicViewMove = true;
					$(".colorIdeaView .rollDiv .list").each(function(q){
						TweenMax.to($(".colorIdeaView .rollDiv .list").eq(q), 0.3, {left:parseInt($(".colorIdeaView .rollDiv .list").eq(q).css("left").split("p")[0]) - 219, ease:Power3.easeOut, onComplete:function(){
							isColorPicViewMove = false;
							if(q == 0) colorPicViewDetachFirst = $(".colorIdeaView .rollDiv .list").eq(0).detach();
							$(".colorIdeaView .rollDiv").append(colorPicViewDetachFirst);
							$(".colorIdeaView .rollDiv .list:last").css("left", 219 * colorPicViewColorMax);
						}});
					});
				}
			} else {
				if(!isColorPicViewMove) {
					isColorPicViewMove = true;
					colorPicViewDetachLast = $(".colorIdeaView .rollDiv .list:last").detach();
					$(".colorIdeaView .rollDiv").prepend(colorPicViewDetachLast);
					$(".colorIdeaView .rollDiv .list:first").css("left", -219);
					$(".colorIdeaView .rollDiv .list").each(function(q){
						TweenMax.to($(".colorIdeaView .rollDiv .list").eq(q), 0.3, {left:parseInt($(".colorIdeaView .rollDiv .list").eq(q).css("left").split("p")[0]) + 219, ease:Power3.easeOut, onComplete:function(){
							isColorPicViewMove = false;
						}});
					});
				}
			}
		});
	});
	

