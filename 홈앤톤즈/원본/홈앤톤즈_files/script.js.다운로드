	//store 매장안내 롤링
	var mainRollN = 0;
	var maxRollN = $(".storeBanner .rollDiv .rollD").size()-1;
	var isStoreRollMove2 = false, firstStoreRollDetach2, lastStoreRollDetach2, firstStoreRollDetach3, lastStoreRollDetach3;
	var storeScrollInterval, storeScrollTime = 100, storeRollImpossible = false;
	/*
	$(".storeBanner .rollDiv .rollD").each(function(index){
		$(".storeBanner .rollDiv .rollD").eq(index).css("left", index*1920);
	});
	$(".storeBanner .introBar .txt").each(function(index){
		$(".storeBanner .introBar .txt").eq(index).css("left", index*900);
	});
	*/

	var storeRollImpossible = false, storeRollInterval, storeRollTime = 6000;
	if($(".storeBanner .rollD").size()-1 <= 0) storeRollImpossible = true;
	storeRollInterval = setInterval(function(){
		if(storeRollImpossible) return;
		TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0.5, {left:"-100%", ease:Power3.easeOut});
		TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0.5, {left:"-100%", ease:Power3.easeOut});
		$(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace("_on.png", ".png"));
		mainRollN++;
		if(mainRollN > $(".storeBanner .bltDiv .bltBt").size()-1) mainRollN = 0;
		TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0, {left:"100%", ease:Power3.easeOut});
		TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0.5, {left:0, ease:Power3.easeOut});
		TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0, {left:"100%", ease:Power3.easeOut});
		TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0.5, {left:0, ease:Power3.easeOut});
		$(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace(".png", "_on.png"));
	}, storeRollTime);

	$(".storeBanner .bltDiv .playBt").click(function(){
		if($(this).find("img").attr("src").indexOf("stop") > -1) {
			$(this).find("img").attr("src", $(this).find("img").attr("src").replace("_stop", "_play"));
			$(this).find("img").attr("alt", "이벤트배너 재생시키기");
			clearInterval(storeRollInterval);
		} else {
			$(this).find("img").attr("src", $(this).find("img").attr("src").replace("_play", "_stop"));
			$(this).find("img").attr("alt", "이벤트배너 정지시키기");
			clearInterval(storeRollInterval);
			storeRollInterval = setInterval(function(){
				TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0.5, {left:"-100%", ease:Power3.easeOut});
				TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0.5, {left:"-100%", ease:Power3.easeOut});
				$(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace("_on.png", ".png"));
				$(".storeBanner .introBar .txt").css("z-index","1");
				$(".storeBanner .rollD").css("z-index","1");
				mainRollN++;
				if(mainRollN > $(".storeBanner .bltDiv .bltBt").size()-1) mainRollN = 0;
				TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0, {left:"100%", ease:Power3.easeOut});
				TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0.5, {left:0, ease:Power3.easeOut});
				TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0, {left:"100%", ease:Power3.easeOut});
				TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0.5, {left:0, ease:Power3.easeOut});
				$(".storeBanner .introBar .txt").eq(mainRollN).css("z-index","2");
				$(".storeBanner .rollD").eq(mainRollN).css("z-index","2");
				$(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace(".png", "_on.png"));
			}, storeRollTime);
		}
	});

	$(".storeBanner .rollBtn > a").each(function(index){
		$(this).click(function(){
			if(!index) {
				//왼쪽
				if(!isStoreRollMove2) {
					isStoreRollMove2 = true;
					TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0.5, {left:"100%", ease:Power3.easeOut});
					TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0.5, {left:"100%", ease:Power3.easeOut});
					$(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace("_on.png", ".png"));
					$(".storeBanner .introBar .txt").css("z-index","1");
					$(".storeBanner .rollD").css("z-index","1");
					mainRollN--;
					if(mainRollN < 0) mainRollN = maxRollN;
					TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0, {left:"-100%", ease:Power3.easeOut});
					TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0, {left:"-100%", ease:Power3.easeOut});
					TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0.5, {left:0, ease:Power3.easeOut});
					$(".storeBanner .introBar .txt").eq(mainRollN).css("z-index","2");
					$(".storeBanner .rollD").eq(mainRollN).css("z-index","2");
					TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0.5, {left:0, ease:Power3.easeOut, onComplete: function(){
						isStoreRollMove2 = false;
					}});
					$(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace(".png", "_on.png"));
				}
				clearInterval(storeRollInterval);
				storeRollInterval = setInterval(function(){
					TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0.5, {left:"-100%", ease:Power3.easeOut});
					TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0.5, {left:"-100%", ease:Power3.easeOut});
					$(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace("_on.png", ".png"));
					$(".storeBanner .introBar .txt").css("z-index","1");
					$(".storeBanner .rollD").css("z-index","1");
					mainRollN++;
					if(mainRollN > $(".storeBanner .bltDiv .bltBt").size()-1) mainRollN = 0;
					TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0, {left:"100%", ease:Power3.easeOut});
					TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0.5, {left:0, ease:Power3.easeOut});
					TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0, {left:"100%", ease:Power3.easeOut});
					TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0.5, {left:0, ease:Power3.easeOut});
					$(".storeBanner .introBar .txt").eq(mainRollN).css("z-index","2");
					$(".storeBanner .rollD").eq(mainRollN).css("z-index","2");
					$(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace(".png", "_on.png"));
				}, storeRollTime);
			} else {
				//오른쪽
				if(!isStoreRollMove2) {
					isStoreRollMove2 = true;
					TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0.5, {left:"-100%", ease:Power3.easeOut});
					TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0.5, {left:"-100%", ease:Power3.easeOut});
					$(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace("_on.png", ".png"));
					$(".storeBanner .introBar .txt").css("z-index","1");
					$(".storeBanner .rollD").css("z-index","1");
					mainRollN++;
					if(mainRollN > maxRollN) mainRollN = 0;
					TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0, {left:"100%", ease:Power3.easeOut});
					TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0, {left:"100%", ease:Power3.easeOut});
					TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0.5, {left:0, ease:Power3.easeOut});
					$(".storeBanner .introBar .txt").eq(mainRollN).css("z-index","2");
					$(".storeBanner .rollD").eq(mainRollN).css("z-index","2");
					TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0.5, {left:0, ease:Power3.easeOut, onComplete: function(){
						isStoreRollMove2 = false;
					}});
					$(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace(".png", "_on.png"));
				}
			}
			clearInterval(storeRollInterval);
			storeRollInterval = setInterval(function(){
				TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0.5, {left:"-100%", ease:Power3.easeOut});
				TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0.5, {left:"-100%", ease:Power3.easeOut});
				$(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace("_on.png", ".png"));
				$(".storeBanner .introBar .txt").css("z-index","1");
				$(".storeBanner .rollD").css("z-index","1");
				mainRollN++;
				if(mainRollN > $(".storeBanner .bltDiv .bltBt").size()-1) mainRollN = 0;
				TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0, {left:"100%", ease:Power3.easeOut});
				TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0.5, {left:0, ease:Power3.easeOut});
				TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0, {left:"100%", ease:Power3.easeOut});
				TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0.5, {left:0, ease:Power3.easeOut});
				$(".storeBanner .introBar .txt").eq(mainRollN).css("z-index","2");
				$(".storeBanner .rollD").eq(mainRollN).css("z-index","2");
				$(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace(".png", "_on.png"));
			}, storeRollTime);
		});
	});
	
	//매장안내 버튼
	$(".storeBanner .bltDiv .bltBt").each(function(q){
		$(this).click(function(){
			if(q != mainRollN){
				TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0.5, {left:"-100%", ease:Power3.easeOut});
				TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0.5, {left:"-100%", ease:Power3.easeOut});
				$(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace("_on.png", ".png"));
				mainRollN = q;
				TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0, {left:"100%", ease:Power3.easeOut});
				TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0, {left:"100%", ease:Power3.easeOut});
				TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0.5, {left:0, ease:Power3.easeOut});
				TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0.5, {left:0, ease:Power3.easeOut});
				$(".storeBanner .bltDiv .bltBt").eq(q).find("img").attr("src", $(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace(".png", "_on.png"));
			}
			clearInterval(storeRollInterval);
			storeRollInterval = setInterval(function(){
				TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0.5, {left:"-100%", ease:Power3.easeOut});
				TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0.5, {left:"-100%", ease:Power3.easeOut});
				$(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace("_on.png", ".png"));
				$(".storeBanner .introBar .txt").css("z-index","1");
				$(".storeBanner .rollD").css("z-index","1");
				mainRollN++;
				if(mainRollN > $(".storeBanner .bltDiv .bltBt").size()-1) mainRollN = 0;
				TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0, {left:"100%", ease:Power3.easeOut});
				TweenMax.to($(".storeBanner .rollD").eq(mainRollN), 0.5, {left:0, ease:Power3.easeOut});
				TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0, {left:"100%", ease:Power3.easeOut});
				TweenMax.to($(".storeBanner .introBar .txt").eq(mainRollN), 0.5, {left:0, ease:Power3.easeOut});
				$(".storeBanner .introBar .txt").eq(mainRollN).css("z-index","2");
				$(".storeBanner .rollD").eq(mainRollN).css("z-index","2");
				$(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src", $(".storeBanner .bltDiv .bltBt").eq(mainRollN).find("img").attr("src").replace(".png", "_on.png"));
			}, storeRollTime);
		});
	});
	

