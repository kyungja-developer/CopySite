	var agreeNum = -1;
	$(".agreeBox .agreeTab a").each(function(e){
		$(this).click(function(){
			if(agreeNum != e){
				//$(".agreeBox .agreeTab a").removeClass("on");
				//$(".agreeBox .agreeTab a").next(".agreeTxt").slideUp(300);
				agreeNum = e;
				$(".agreeBox .agreeTab a").eq(agreeNum).addClass("on");
				$(".agreeBox .agreeTab a").eq(agreeNum).next(".agreeTxt").slideDown(300);
			}else{
				$(".agreeBox .agreeTab a").eq(agreeNum).removeClass("on");
				$(".agreeBox .agreeTab a").eq(agreeNum).next(".agreeTxt").slideUp(300);
				agreeNum = -1;
			}
		
		});
	
