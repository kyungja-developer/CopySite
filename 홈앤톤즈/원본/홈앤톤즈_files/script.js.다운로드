	//시공사례 리스트
	var isMove = false, buildExpRollNum = 0, buildExpInterval, buildExpTime = 3000;
	var detachFirst, detachLast;
	if($(".buildExpDiv1 .rollDiv .roll").size()-1 == 0) {
		$(".buildExpDiv1 .inner .prevBtn").hide();
		$(".buildExpDiv1 .inner .nextBtn").hide();
	}
	$(".buildExpDiv1 .rollDiv").height($(".buildExpDiv1 .rollDiv .roll:first").height());
	$(".buildExpDiv1 .rollDiv .roll").each(function(index){
		$(".buildExpDiv1 .rollDiv .roll").eq(index).css("left", index * 1040);
	});
	$(".buildExpDiv1 .inner .prevBtn").click(function(){
		if(!isMove) {
			isMove = true;
			//$(".buildExpDiv1 .rollDiv").height($(".buildExpDiv1 .rollDiv .roll:eq(1)").height());
			TweenMax.to($(".buildExpDiv1 .rollDiv"), 0.5, {height:$(".buildExpDiv1 .rollDiv .roll:eq(1)").height(), ease:Power3.easeOut});
			$(".buildExpDiv1 .rollDiv .roll").each(function(index){
				TweenMax.to($(".buildExpDiv1 .rollDiv .roll").eq(index), 0.5, {left:parseInt($(".buildExpDiv1 .rollDiv .roll").eq(index).css("left").split("p")[0]) - 1040, ease:Power3.easeOut, onComplete:function(){
					isMove = false;
					if(index == 0) detachFirst = $(".buildExpDiv1 .rollDiv .roll").eq(0).detach();
					$(".buildExpDiv1 .rollDiv").append(detachFirst);
					$(".buildExpDiv1 .rollDiv .roll:last").css("left", 1040 * ($(".buildExpDiv1 .rollDiv .roll").size()-1));
				}});
			});
		}
	});
	$(".buildExpDiv1 .inner .nextBtn").click(function(){
		if(!isMove) {
			isMove = true;
			detachLast = $(".buildExpDiv1 .rollDiv .roll:last").detach();
			$(".buildExpDiv1 .rollDiv").prepend(detachLast);
			$(".buildExpDiv1 .rollDiv .roll:first").css("left", -1040);
			//$(".buildExpDiv1 .rollDiv").height($(".buildExpDiv1 .rollDiv .roll:first").height());
			TweenMax.to($(".buildExpDiv1 .rollDiv"), 0.5, {height:$(".buildExpDiv1 .rollDiv .roll:first").height(), ease:Power3.easeOut});
			$(".buildExpDiv1 .rollDiv .roll").each(function(index){
